{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": ["es6"],
        "allowJs": true,
        "outDir": "build",
        "rootDir": "src",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "downlevelIteration": true,
        "strictNullChecks": false
    },
    "include": ["src/**/*.ts", "src/events/client/InteractionCreate.ts"],
    "exclude": ["src/**/*.spec.ts", "node_modules"]
}
